powerfun <- function(n,a){
function(x) {
pnorm(sqrt(n)*x- qnorm(1-a))}
}
powerfun2 <- function(n,a){
function(x) {
pnorm(sqrt(n)*x- qnorm(1-a))+1-pnorm(sqrt(n)*x- qnorm(a))}
}
plotpower=function(n,a,lower=0,upper=1,twotail=F){
require(ggplot2)
if (twotail==F) {
f=powerfun(n,a)
}
else {
f=powerfun2(n,a)
}
p = ggplot(data = data.frame(x = 0), mapping = aes(x = x))
p=p+stat_function(fun= f,color="blue")+scale_x_continuous(limits = c(lower, upper))+xlab(expression(theta))+ylab("Power")
}
plotpower(64,.05,-1,1)
plotpower=function(n,a,lower=0,upper=1,twotail=F){
require(ggplot2)
if (twotail==F) {
f=powerfun(n,a)
}
else {
f=powerfun2(n,a)
}
p = ggplot(data = data.frame(x = 0), mapping = aes(x = x))
p+stat_function(fun= f,color="blue")+scale_x_continuous(limits = c(lower, upper))+xlab(expression(theta))+ylab("Power")
}
